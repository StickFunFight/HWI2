import TI.BoeBot;
import TI.PinMode;

public class RobotMain {

    public static void main(String[] args) {

        boolean state = true;
//de led knippert voor 0,1 seconde
        while (true) {
            state = !state;
            BoeBot.digitalWrite(0, state);
            BoeBot.wait(100);
        }
    }
}
