import TI.BoeBot;
import TI.PinMode;

public class RobotMain {

    public static void main(String[] args) {

        boolean state = true;

        while (true) {
            state = !state;
//zet de let 1 seconde uit en 1 seconde aan
            BoeBot.digitalWrite(0, state);
            BoeBot.wait(1000);
        }
    }
}
